<% for(var i=0; i<users.length; i++) {%>
    <tr>
        <td><%= users[i].ID %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].groupName %></td>
        <td>
            <a href="#" class="btn border-shadow update" onclick="showPopup()">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
        </td>
    </tr>
    <% } %>

    <script>
        function showPopup() {
            window.open('/authorityManage/user/update', '권한 수정', 'width=400, height=300');
        }
    </script>