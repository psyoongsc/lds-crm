<% for(var i=0; i<users.length; i++) {%>
    <tr>
        <td><%= users[i].ID %></td>
        <td><%= users[i].name %></td>
        <td><%= users[i].phone %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].deptName %></td>
        <td><%= users[i].posName %></td>
        <td>
            <a href="/membermanage/update/<%= users[i].ID %>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" onclick='deleteButton("<%= users[i].ID %>")'>
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>